{
    "name": "mylib7790"
    , "description": "this is my library to learn how to contribute to open source"
    , "main": "dist/index.js"
    , "scripts": {
        "commit": "git-cz"
        , "check-coverage": "nyc check-coverage --statements 100 --branches 100 --functions 100 --lines 100"
        , "report-coverage": "cat ./coverage/lcov.info | codecov"
        , "watch:test": "npm t -- -w"
        , "test": "mocha --compilers js:babel-register"
        , "cover": "nyc npm t"
        , "prebuild": "rimraf dist"
        , "build": "babel --copy-files --out-dir dist --ignore *test.js src"
        , "semantic-release": "semantic-release pre && npm publish && semantic-release post"
    }
    , "repository": {
        "type": "git"
        , "url": "https://github.com/surender7790/mylib.git"
    }
    , "keywords": [
    "javascript"
    , "mocha"
    , "mylib"
    , "travis"
    , "git"
    , "github"
    , "open"
    , "source"
  ]
    , "files": [
    "dist"
    , "README.md"
  ]
    , "author": "'Surender <'surender7790@gmail.com'> (http://surenderkumar.me/)"
    , "license": "MIT"
    , "bugs": {
        "url": "https://github.com/surender7790/mylib/issues"
    }
    , "homepage": "https://github.com/surender7790/mylib#readme"
    , "dependencies": {
        "unique-random-array": "1.0.0"
    }
    , "devDependencies": {
        "babel-cli": "6.16.0"
        , "babel-preset-es2015": "6.16.0"
        , "babel-preset-stage-2": "6.17.0"
        , "babel-register": "6.16.3"
        , "chai": "3.2.0"
        , "codecov.io": "0.1.6"
        , "commitizen": "2.8.6"
        , "cz-conventional-changelog": "1.2.0"
        , "ghooks": "1.3.2"
        , "istanbul": "0.4.5"
        , "mocha": "2.2.0"
        , "nyc": "8.3.1"
        , "rimraf": "2.5.4"
        , "semantic-release": "^4.3.5"
    }
    , "czConfig": {
        "path": "node_modules/cz-conventional-changelog"
    }
    , "config": {
        "ghooks": {
            "pre-commit": "npm run cover && npm run check-coverage"
        }
    }
    , "babel": {
        "presets": [
      "es2015"
      , "stage-2"
    ]
    }
}